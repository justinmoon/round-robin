{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "common": "file:../common",
    "draft-js": "^0.10.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-flexbox-grid": "https://github.com/roylee0704/react-flexbox-grid.git#master",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-query": "^1.2.0",
    "redux-thunk": "^2.2.0",
    "standard": "^9.0.1"
  },
  "devDependencies": {
    "react-scripts": "0.9.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": "http://localhost:5000",
  "scripts": {
    "start": "react-scripts start",
    "release": "cp -r ../common node_modules/ && REACT_APP_BASE_URL=/api react-scripts build && cp _redirects build/_redirects",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/standard",
    "netlify": "npm install && npm run build",
    "postinstall": "(cd node_modules/react-flexbox-grid && npm install)",
    "fix": "./node_modules/.bin/standard --fix"
  }
}
